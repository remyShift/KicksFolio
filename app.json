{
  "expo": {
    "name": "KicksFolio",
    "slug": "KicksFolio-Frontend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "kicksfolio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F27329"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.kicksfolio",
      "associatedDomains": [
        "applinks:share.kicksfolio.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIApplicationSupportsIndirectInputEvents": true,
        "UIApplicationSceneManifest": {
          "UIApplicationSupportsMultipleScenes": false
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.kicksfolio",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "kicksfolio",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "share.kicksfolio.com",
              "pathPrefix": "/share-collection"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "KicksFolio need to access your photos to select existing sneakers photos and add them to your personal collection.",
          "cameraPermission": "KicksFolio need to access your camera to take photos of your sneakers and add them to your personal collection."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "KicksFolio need to access your camera to take photos of your sneakers and add them to your personal collection."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "resizeMode": "contain",
          "imageResizeMode": "contain",
          "imageWidth": 600,
          "imageHeight": 600,
          "backgroundColor": "#F27329"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/icon.png",
          "color": "#F27329"
        }
      ],
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e27c7b67-44d9-4af8-9a57-d94b81359832"
      }
    },
    "owner": "remyshift"
  }
}
