{
  "name": "kicksfolio",
  "scheme": "kicksfolio",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "prebuild": "expo prebuild --clean --platform all",
    "build:all": "eas build --platform all --profile production --clear-cache",
    "build:ios": "eas build --platform ios --profile production --clear-cache",
    "build:android": "eas build --platform android --profile production --clear-cache",
    "build:all:dev": "eas build --platform all --profile development --clear-cache",
    "build:ios:dev": "eas build --platform ios --profile development --clear-cache",
    "build:android:dev": "eas build --platform android --profile development --clear-cache",
    "test": "npm run test:jest && npm run test:vitest",
    "test:jest": "jest --verbose",
    "test:vitest": "NODE_ENV=test vitest run",
    "test:interfaces": "vitest run __tests__/interfaces",
    "test:hooks": "vitest run __tests__/hooks",
    "test:components": "jest --verbose __tests__/screens",
    "test:coverage": "jest --coverage --verbose && vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "prepare": "husky",
    "commit": "cz",
    "madge": "madge --circular --extensions ts,tsx,js,jsx src/",
    "madge:warn": "madge --warning --extensions ts,tsx,js,jsx src/",
    "madge:image": "madge --image graph.svg src/",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "1.7.6",
    "@supabase/supabase-js": "^2.50.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "base64-arraybuffer": "^1.0.2",
    "chalk": "^5.4.0",
    "expo": "53.0.22",
    "expo-application": "^6.1.5",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.2.1",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-crop-picker": "^0.50.1",
    "react-native-keyboard-controller": "^1.17.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.1",
    "react-native-unistyles": "^2.43.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "unique-filename": "^4.0.0",
    "zod": "^3.25.57",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "latest",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/expo": "^32.0.13",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "jest-react-native": "^18.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,css,md}": "prettier --write"
  },
  "jest": {
    "preset": "jest-expo",
    "maxWorkers": "50%",
    "testTimeout": 5000,
    "setupFilesAfterEnv": [
      "./src/__tests__/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/screens/**/*.(test|spec).(js|jsx|ts|tsx)",
      "**/__tests__/modals/**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "__tests__/setup.ts",
      "__tests__/vitest-setup.ts",
      "__tests__/interfaces/",
      "__tests__/hooks/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|native-base|react-native-svg|@supabase/.*|isows|ws)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**",
      "!**/__tests__/**"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
