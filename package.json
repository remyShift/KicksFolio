{
  "name": "kicksfolio",
  "scheme": "kicksfolio",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "build:all": "eas build --platform all --profile production --clear-cache",
    "build:ios": "eas build --platform ios --profile production --clear-cache",
    "build:android": "eas build --platform android --profile production --clear-cache",
    "build:all:dev": "eas build --platform all --profile development --clear-cache",
    "build:ios:dev": "eas build --platform ios --profile development --clear-cache",
    "build:android:dev": "eas build --platform android --profile development --clear-cache",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --verbose",
    "test:ci": "jest --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:quick": "jest --bail --findRelatedTests",
    "test:watch": "jest --watch",
    "lint": "expo lint",
    "prepare": "husky",
    "commit": "cz"
  },
  "dependencies": {
    "@expo/config-plugins": "~10.1.1",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.50.0",
    "base64-arraybuffer": "^1.0.2",
    "chalk": "^5.4.0",
    "expo": "53.0.20",
    "expo-application": "^6.1.5",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.2.1",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-crop-picker": "^0.50.1",
    "react-native-keyboard-controller": "^1.17.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.1",
    "react-native-unistyles": "^2.43.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "unique-filename": "^4.0.0",
    "zod": "^3.25.57",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "latest",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/expo": "^32.0.13",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "19.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./__tests__/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "__tests__/setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@supabase/.*|isows|ws)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**",
      "!**/__tests__/**"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
