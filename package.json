{
  "name": "kicksfolio",
  "scheme": "kicksfolio",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "prebuild": "expo prebuild --clean --platform all",
    "build:all": "eas build --platform all --profile production --clear-cache",
    "build:ios": "eas build --platform ios --profile production --clear-cache",
    "build:android": "eas build --platform android --profile production --clear-cache",
    "build:all:dev": "eas build --platform all --profile development --clear-cache",
    "build:ios:dev": "eas build --platform ios --profile development --clear-cache",
    "build:android:dev": "eas build --platform android --profile development --clear-cache",
    "test": "npm run test:jest && npm run test:vitest",
    "test:jest": "jest --verbose",
    "test:vitest": "NODE_ENV=test vitest run",
    "test:interfaces": "vitest run __tests__/interfaces",
    "test:hooks": "vitest run __tests__/hooks",
    "test:components": "jest --verbose __tests__/screens",
    "test:coverage": "jest --coverage --verbose && vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "prepare": "husky",
    "commit": "cz",
    "madge": "madge --circular --extensions ts,tsx,js,jsx src/",
    "madge:warn": "madge --warning --extensions ts,tsx,js,jsx src/",
    "madge:image": "madge --image graph.svg src/",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "update": "eas update --auto",
    "update:dev": "eas update --branch development --message \"Dev update\"",
    "update:master": "eas update --branch master --message \"Production update from master\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.50.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "base64-arraybuffer": "^1.0.2",
    "chalk": "^5.4.0",
    "expo": "54.0.12",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.13",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.14",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-store-review": "~9.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "i18next": "^25.2.1",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-crop-picker": "^0.50.1",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.1",
    "react-native-unistyles": "^2.43.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17",
    "unique-filename": "^4.0.0",
    "vexo-analytics": "^1.5.1",
    "zod": "^3.25.57",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-native-community/cli": "latest",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/expo": "^32.0.13",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "jest-react-native": "^18.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,css,md}": "prettier --write"
  },
  "jest": {
    "preset": "jest-expo",
    "maxWorkers": "50%",
    "testTimeout": 5000,
    "setupFilesAfterEnv": [
      "./src/__tests__/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/screens/**/*.(test|spec).(js|jsx|ts|tsx)",
      "**/__tests__/modals/**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "__tests__/setup.ts",
      "__tests__/vitest-setup.ts",
      "__tests__/interfaces/",
      "__tests__/hooks/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|native-base|react-native-svg|@supabase/.*|isows|ws)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**",
      "!**/__tests__/**"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
